#include<stdio.h>
#include<stdlib.h>
#include<string.h>

int main(){
	FILE *r = fopen("input.txt", "r");
	if (r == NULL){
		printf("u looser\n");
		return 0;
	}
	char ans[10000], cur[100];
	int b = 0;
	int i = 0;
	int j = 0;
	while (fgets(cur, 255, r)){
		int i;
		for (i = 1; i < strlen(cur); ++i){
		  	if (b == 1){
		  		ans[j] = cur[i];
		  		j++;
	  		}
			if (cur[i] == '/' && cur[i-1] == '/'){
				b = 1;
			}	  
		}
		if (b == 1){
			ans[j] = '\n';
			j++;
		}
		b = 0;
	}
	fclose(r);
	ans[j] = -1;
	i = 0;
	FILE *w = fopen("output.txt", "w");
	if (w == NULL){
		printf("u looser\n");
		return 0;
	}
	while (ans[i] != EOF){
		fprintf(w, "%c", ans[i]);
		++i;
	}
	fclose(w);
	
}
